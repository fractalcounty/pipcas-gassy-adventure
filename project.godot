; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PGA"
run/main_scene="res://boot.tscn"
config/features=PackedStringArray("4.0", "Mobile")
run/max_fps=120
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://assets/video/default_bus_layout.tres"

[autoload]

Events="*res://src/singletons/events.gd"
Global="*res://src/singletons/global.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
UIAudio="*res://src/singletons/ui_audio.tscn"
Music="*res://src/singletons/music.tscn"
InputBuffer="*res://src/singletons/input_buffer.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1440
window/size/window_height_override=2560
window/stretch/aspect="expand"

[editor]

script/templates_search_path="res://addons/xsm/templates/"

[editor_plugins]

enabled=PackedStringArray("res://addons/random_audio_player/plugin.cfg", "res://addons/shaker/plugin.cfg", "res://addons/nklbdev.previewing_parallax_background/plugin.cfg", "res://addons/nklbdev.parallax/plugin.cfg", "res://addons/better-terrain/plugin.cfg", "res://addons/trail_2d/plugin.cfg", "res://addons/smoothing/plugin.cfg", "res://addons/AS2P/plugin.cfg", "res://addons/acrylic_glass/plugin.cfg", "res://addons/editor_icon_previewer/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg", "res://addons/monitor_overlay/plugin.cfg")

[gui]

theme/custom_font="res://assets/themes/fonts/HelveticaNowText-Regular.ttf"
theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true
common/snap_controls_to_pixels=false

