; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PGA"
run/main_scene="res://scenes/boot/boot.tscn"
config/features=PackedStringArray("4.0", "Mobile")
run/max_fps=120
config/icon="res://icon.svg"

[autoload]

Events="*res://src/singletons/events.gd"
Global="*res://src/singletons/global.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
UIAudio="*res://src/singletons/ui_audio.tscn"
Music="*res://src/singletons/music.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=3200
window/size/window_height_override=1800
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"

[editor]

script/templates_search_path="res://addons/xsm/templates/"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/xsm/plugin.cfg", "res://addons/random_audio_player/plugin.cfg", "res://addons/shaker/plugin.cfg", "res://addons/nklbdev.previewing_parallax_background/plugin.cfg", "res://addons/nklbdev.parallax/plugin.cfg", "res://addons/codeandweb.texturepacker/plugin.cfg", "res://addons/better-terrain/plugin.cfg", "res://addons/trail_2d/plugin.cfg")

[gui]

theme/custom_font="res://assets/fonts/BerkeleyMonoTrial-Regular.otf"
theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true

